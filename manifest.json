{
    "manifest_version": 2,
    "name": "subtle text",
    "description": "perform subtle text manipulations on browers content",
    "version": "0.0.1",
    "browser_action": {
        "default_popup": "menu.html"
    },
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["subtle.js"],
            "run_at":"document_end"
        }
    ]
}
